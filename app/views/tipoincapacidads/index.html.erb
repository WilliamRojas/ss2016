<h1>Listing tipoincapacidads</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @tipoincapacidads.each do |tipoincapacidad| %>
      <tr>
        <td><%= tipoincapacidad.nombre %></td>
        <td><%= link_to 'Show', tipoincapacidad %></td>
        <td><%= link_to 'Edit', edit_tipoincapacidad_path(tipoincapacidad) %></td>
        <td><%= link_to 'Destroy', tipoincapacidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipoincapacidad', new_tipoincapacidad_path %>
