<h1>Listing tipoincidentes</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @tipoincidentes.each do |tipoincidente| %>
      <tr>
        <td><%= tipoincidente.nombre %></td>
        <td><%= link_to 'Show', tipoincidente %></td>
        <td><%= link_to 'Edit', edit_tipoincidente_path(tipoincidente) %></td>
        <td><%= link_to 'Destroy', tipoincidente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipoincidente', new_tipoincidente_path %>
