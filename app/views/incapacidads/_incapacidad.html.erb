<tr id=<%= "incapacidad_#{incapacidad.id}" %>> 
  
      <td><%= incapacidad.funcionario.nombres rescue nil %></td>
      <td><%= mostrartipoincapacidad(incapacidad.tipoincapacidad_id) %></td>
      <td><%= incapacidad.fechainicio %></td>
      <td><%= incapacidad.fechaterminacion %></td>
      <td><%= incapacidad.totaldias %></td>
      <td><%= incapacidad.diasacobrar %></td>
      <td><%= incapacidad.prorroga %></td>
      <td><%= incapacidad.numeroinpacacidad %></td>
      <td><%= incapacidad.origendelaincapacidad %></td>

      <% if can? :read, incapacidad%>
      <td><%= link_to 'Ver', incapacidad, remote: true,  class: "btn btn-success btn-xs" %></td>
      <%end%>
      <% if can? :update, incapacidad%>
      <td><%= link_to 'Editar', edit_incapacidad_path(incapacidad), remote: true, class: "btn btn-warning btn-xs" %></td>
      <%end%>
      <% if can? :destroy, incapacidad%>
      <td><%= link_to 'Eliminar', incapacidad, method: :delete, data: { confirm: 'Esta Seguro?' }, class: "btn btn-danger btn-xs", remote: true %></td>
      <%end%>
</tr>