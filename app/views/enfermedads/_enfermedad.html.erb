<tr id=<%= "enfermedad_#{enfermedad.id}" %>> 
   
	<td><%= enfermedad.funcionario.nombres rescue nil %></td>
	<td><%= enfermedad.tipoenfermedad.nombre rescue nil %></td>
	<td><%= enfermedad.descripcion %></td>
	<td><%= enfermedad.fecha %></td>
	<td><%= enfermedad.codigodediagnostico %></td>
   
   <% if can? :read, enfermedad%>
    <td><%= link_to 'Ver', enfermedad, remote: true, class: "btn btn-success btn-xs" %></td>
    <%end%>
    <% if can? :update, enfermedad%>
    <td><%= link_to 'Editar', edit_enfermedad_path(enfermedad), remote: true, class: "btn btn-warning btn-xs" %></td>
    <%end%>
     <% if can? :destroy, enfermedad%>
    <td><%= link_to 'Eliminar', enfermedad, method: :delete, data: { confirm: 'Esta Seguro?' }, class: "btn btn-danger btn-xs", remote: true %></td>
    <%end%>
</tr>